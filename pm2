# how to run pm2 in win10
  - npm install -g pm2 
  - pm2 status
  - pm2 start --name "file name" 
  - npm install pm2-windows-startup -g
  - pm2-startup install
  - pm2 save
  - pm2 resurrect
 
# create fiel.bat
   TIMEOUT 8

   pm2 restart 1
 
   cmd /K
# run on file
  TIMEOUT = 5
  D:
  CD /IMS/ims/abs_ko
  NPM start
  cmd /k
